{
  "version": "1.6.0",
  "learner": {
    "gradient_booster": {
      "model": {
        "trees": [
          {
            "id": 0,
            "depth": 4,
            "split": {
              "feature": "sales_volume",
              "threshold": 30000,
              "yes": {
                "split": {
                  "feature": "sales_volume",
                  "threshold": 45000,
                  "yes": { "leaf": 0.10 },
                  "no": {
                    "split": {
                      "feature": "receipt_count",
                      "threshold": 300,
                      "yes": { "leaf": 0.20 },
                      "no": { "leaf": 0.35 }
                    }
                  }
                }
              },
              "no": {
                "split": {
                  "feature": "sales_volume",
                  "threshold": 15000,
                  "yes": {
                    "split": {
                      "feature": "receipt_count",
                      "threshold": 200,
                      "yes": { "leaf": 0.50 },
                      "no": { "leaf": 0.70 }
                    }
                  },
                  "no": { "leaf": 0.90 }
                }
              }
            }
          },
          {
            "id": 1,
            "depth": 3,
            "split": {
              "feature": "transaction_drop",
              "threshold": -10,
              "yes": { "leaf": 0.60 },
              "no": {
                "split": {
                  "feature": "traffic_change",
                  "threshold": -10,
                  "yes": { "leaf": 0.45 },
                  "no": { "leaf": 0.25 }
                }
              }
            }
          },
          {
            "id": 2,
            "depth": 3,
            "split": {
              "feature": "peak_hours_revenue",
              "threshold": 40,
              "yes": { "leaf": 0.20 },
              "no": {
                "split": {
                  "feature": "historical_trend",
                  "threshold": 0,
                  "yes": { "leaf": 0.40 },
                  "no": { "leaf": 0.30 }
                }
              }
            }
          },
          {
            "id": 3,
            "depth": 2,
            "split": {
              "feature": "transactions",
              "threshold": 300,
              "yes": { "leaf": 0.20 },
              "no": { "leaf": 0.45 }
            }
          },
          {
            "id": 4,
            "depth": 2,
            "split": {
              "feature": "daily_revenue",
              "threshold": 40000,
              "yes": { "leaf": 0.15 },
              "no": { "leaf": 0.55 }
            }
          }
        ]
      }
    }
  },
  "objective": "binary:logistic"
}
